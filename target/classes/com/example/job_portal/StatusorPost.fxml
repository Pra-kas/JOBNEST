<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.job_portal.Statusorpost">
    <children>
        <Button fx:id="JobPost" layoutX="292.0" layoutY="318.0" mnemonicParsing="false" onAction="#HandleJobPost" prefHeight="37.0" prefWidth="98.0" style="-fx-background-color: #1D2026; -fx-background-radius: 10;" text="JobPost" textFill="#fcfafa">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Button fx:id="requests" layoutX="292.0" layoutY="183.0" mnemonicParsing="false" onAction="#HandleRequest" prefHeight="37.0" prefWidth="98.0" style="-fx-background-color: #1D2026; -fx-background-radius: 10;" text="Requests" textFill="#f8f1f1">
            <font>
                <Font size="16.0" />
            </font>
        </Button>
        <Button fx:id="editprofile" layoutX="292.0" layoutY="56.0" mnemonicParsing="false" onAction="#HandleEditProfile" prefHeight="37.0" prefWidth="98.0" style="-fx-background-color: #1D2026; -fx-background-radius: 10;" text="EditProfie" textFill="#fcfcfc">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <AnchorPane prefHeight="404.0" prefWidth="133.0" style="-fx-background-color: #1D2026;" />
        <ImageView fitHeight="93.0" fitWidth="91.0" layoutX="184.0" layoutY="31.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@editprofile.jpg" />
            </image>
        </ImageView>
        <ImageView fitHeight="93.0" fitWidth="91.0" layoutX="176.0" layoutY="290.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@request.jpg" />
            </image>
        </ImageView>
        <ImageView fitHeight="93.0" fitWidth="91.0" layoutX="184.0" layoutY="162.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@post.jpg" />
            </image>
        </ImageView>
      <Button fx:id="backbutton" layoutX="150.0" layoutY="14.0" mnemonicParsing="false" onAction="#HandleBack" text="Back" />
    </children>
    <Button fx:id="logoutid" layoutX="489.0" layoutY="14.0" mnemonicParsing="false" onAction="#HandleLogout" prefHeight="26.0" prefWidth="70.0" text="logout" />
</AnchorPane>
